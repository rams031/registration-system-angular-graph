div.basisHalf.bg-white.h-full.flex.justify-center.items-center.px-4.animate__animated.animate__fadeIn
  //- form(role="form" [formGroup]="profileForm" (ngSubmit)="onSubmit()")
  form.form.w-96((ngSubmit)="onSubmit($event)" #myForm="ngForm")
    //- div.p-4.mb-4.text-sm.text-red-800.rounded-lg.bg-red-50.text-center(role="alert")
    //- span.font-medium Invalid Credential. Change a few things up and try submitting again.
    div.flex.justify-center.items-center.text-violet-600.font-bold.text-xl.py-2 Sign In
    div.flex.flex-col.p-2
      label.text-sm.font-bold.text-violet-800.p-1 Username 
      span.text-sm 
      input.formInput([(ngModel)]="userCredential.username" type="text" name="username" required)
    div.flex.flex-col.p-2
      label.text-sm.font-bold.text-violet-800.p-1 Password
      input.formInput([(ngModel)]="userCredential.password" type="password" name="password" required)
    div.flex.justify-between.items-center.px-2
    div.flex.justify-start.items-center
      a.formInputHover.bg-white.shadow.shadow-indigo-200.m-3.rounded.py-2.px-5.font-bold.text-violet-800(href="/register") Register
      button.formInputHover.bg-white.shadow.shadow-indigo-200.m-3.rounded.py-2.px-5.font-bold.text-violet-800(type="submit") Submit
    div
      li(*ngFor="let item of accountData$ | async") Username - {{item.username}} - Password {{item.password}}

